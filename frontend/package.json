{
  "name": "ethereum-dapp-starter-kit",
  "version": "0.0.1",
  "description": "Frontend starter kit for Ethereum DApps",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:sandbox",
    "dev:beta": "npm run codegen:beta && better-npm-run dev",
    "dev:beta-defi": "npm run codegen:beta-defi && cross-env MODE=beta-defi better-npm-run dev",
    "dev:sandbox": "npm run codegen:sandbox && cross-env MODE=sandbox better-npm-run dev",
    "build:beta": "better-npm-run build",
    "build:beta-defi": "cross-env MODE=beta-defi better-npm-run build",
    "build:sandbox": "cross-env MODE=sandbox better-npm-run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit --pretty",
    "lint:autofix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:coverage": "jest --coverage --watch",
    "codegen:beta": "graphql-codegen --config codegen-beta.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json",
    "codegen:beta-defi": "graphql-codegen --config codegen-beta-defi.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json",
    "codegen:sandbox": "graphql-codegen --config codegen-sandbox.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json"
  },
  "betterScripts": {
    "node": {
      "command": "node --max_old_space_size=4096 ./node_modules/.bin/cross-env",
      "env": {
        "TS_NODE_PROJECT": "webpack/tsconfig.json",
        "TS_NODE_FILES": true
      }
    },
    "build": {
      "command": "better-npm-run node webpack --config webpack/prod.ts"
    },
    "dev": {
      "command": "better-npm-run node webpack-dev-server --open --config webpack/dev.ts"
    }
  },
  "author": "Serdtsev Dmitriy",
  "license": "MIT",
